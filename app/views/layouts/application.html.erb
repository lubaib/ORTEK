	<style type="text/css">
	.animated { visibility:hidden; !important;}
	#testimonials {
	    background: none repeat scroll 0 0 #eee !important;
	    min-height: 400px;
	    position: relative;
	}
	#testimonials .testimonials-inner {
    	height: 100%;
    	padding-bottom: 60px;
    	position: relative;
    	width: 100%;
	}
	#testimonials .testimonials-inner .testimonial-container {
	    margin-top: 20px;
	    position: relative;
	}
	.text-right {
	    text-align: right;
	}
	#testimonials .testimonials-inner .testimonial-container .testimonial .testi-img {
	    display: block;
	    height: 112px;
	    margin-left: 100px;
	    margin-top: 18px;
	    position: relative;
	    width: 112px;
	    z-index: 3;
	}
	#testimonials .testimonials-inner .testimonial-container .testimonial .testi-text:before {
	    -moz-border-bottom-colors: none;
	    -moz-border-left-colors: none;
	    -moz-border-right-colors: none;
	    -moz-border-top-colors: none;
	    border-color: transparent #ffffff transparent transparent;
	    border-image: none;
	    border-radius: 30px;
	    border-style: solid;
	    border-width: 50px;
	    content: "";
	    display: none;
	    left: 0;
	    position: absolute;
	    top: 0;
	}
	#testimonials .testimonials-inner .testimonial-container .testimonial .testi-img img {
	    -moz-border-bottom-colors: none;
	    -moz-border-left-colors: none;
	    -moz-border-right-colors: none;
	    -moz-border-top-colors: none;
	    border-color: #66fffc transparent #66fffc #66fffc;
	    border-image: none;
	    border-radius: 55px;
	    border-style: solid;
	    border-width: 2px;
	    display: block;
	    position: relative;
	}
	#testimonials .testimonials-inner .testimonial-container .testimonial .testi-text {
	    border: 2px solid #fed136;
	    border-radius: 30px;
	    color: #777;
	    padding: 20px 10px 10px 25px;
	    position: relative;
	    transition: all 600ms ease-in 0s;
	    z-index: 2;
	}
	.one_half img{
		width: 400px;
		height: 400px;
	}
	.one_half img:hover{
		opacity: 0.4;
		filter: alpha(opacity=40);
	}
	</style>
		
